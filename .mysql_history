SELECT\040concat(UPPER(SUBSTRING(au_lname\040FROM\0401\040FOR\0403))\040,"",\040SUBSTRING(phone\040FROM\0409))\040\040AS\040"Search\040Id",\040concat(au_lname,\040',\040'\040,au_fname)\040\040AS\040"name"\040FROM\040authors;
SELECT\040au_lname\040\040AS\040"au_lname"\040,CHARACTER_LENGTH(au_lname)\040\040AS\040"length"\040FROM\040authors\040\040ORDER\040BY\040au_lname\040ASC;
SELECT\040title_id,title_name,EXTRACT(YEAR\040FROM\040pubdate)\040AS\040year\040From\040titles;
SELECT\040title_id,title_name,pubdate,concat(EXTRACT(YEAR\040FROM\040pubdate)\040+\04028,"-",SUBSTRING(pubdate\040FROM\0406\040FOR\0405))\040AS\040"copyright-date"\040From\040titles;
SELECT\040\040title_id,\040\040title_name,\040\040price,\040\040CASE\040type\040\040WHEN\040'history'\040\040THEN\040price\040*\040.9\040\040ELSE\040price\040*\040.8\040\040END\040\040AS\040"New\040price"\040\040FROM\040titles\040\040;
SELECT\040title_id,\040title_name,
\040\040\040\040\040\040\040CASE
\040\040\040\040\040\040\040WHEN\040price\040IS\040NULL
\040\040\040\040\040\040\040THEN\040"priceless"
\040\040\040\040\040\040\040ELSE\040price
\040\040\040\040\040\040\040END\040AS\040price\040FROM\040titles;
SELECT\040title_id,\040title_name,\040\040\040\040\040\040\040\040CASE\040\040\040\040\040\040\040\040WHEN\040price\040IS\040NULL\040\040\040\040\040\040\040\040THEN\040"priceless"\040\040\040\040\040\040\040\040ELSE\040price\040\040\040\040\040\040\040\040END\040AS\040price\040FROM\040titles;
SELECT\040\040CURRENT_TIMESTAMP\040AS\040"Current\040Time";
SELECT\040CURRENT_USER\040AS\040"server";
exit
use\040books;
tee\040lab_functions.txt
SELECT\040title_id,
\040price,
\0400.10\040AS\040"Discount",
\040price\040*\040(1\040-\0400.10)\040AS\040"New\040price"
\040FROM\040titles;
